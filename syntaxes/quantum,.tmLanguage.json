{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Quantum",
    "fileTypes": [
        "axes",
        "qin"
    ],
    "scopeName": "source.quantum",
    "patterns": [
        {
            "comment": "boxed slice literal",
            "begin": "(<)(\\[)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.brackets.angle.quantum"
                },
                "2": {
                    "name": "punctuation.brackets.square.quantum"
                }
            },
            "end": ">",
            "endCaptures": {
                "0": {
                    "name": "punctuation.brackets.angle.quantum"
                }
            },
            "patterns": [
                {
                    "include": "#comments"
                },
                {
                    "include": "#gtypes"
                },
                {
                    "include": "#lvariables"
                },
                {
                    "include": "#lifetimes"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                }
            ]
        },
        {
            "comment": "macro type metavariables",
            "name": "meta.macro.metavariable.type.quantum",
            "match": "(\\$)((crate)|([A-Z][A-Za-z0-9_]*))((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.quantum"
                },
                "3": {
                    "name": "keyword.other.crate.quantum"
                },
                "4": {
                    "name": "entity.name.type.metavariable.quantum"
                },
                "6": {
                    "name": "keyword.operator.key-value.quantum"
                },
                "7": {
                    "name": "variable.other.metavariable.specifier.quantum"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro metavariables",
            "name": "meta.macro.metavariable.quantum",
            "match": "(\\$)([a-z][A-Za-z0-9_]*)((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.quantum"
                },
                "2": {
                    "name": "variable.other.metavariable.name.quantum"
                },
                "4": {
                    "name": "keyword.operator.key-value.quantum"
                },
                "5": {
                    "name": "variable.other.metavariable.specifier.quantum"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro rules",
            "name": "meta.macro.rules.quantum",
            "match": "\\b(macro_rules!)\\s+(([a-z0-9_]+)|([A-Z][a-z0-9_]*))\\s+(\\{)",
            "captures": {
                "1": {
                    "name": "entity.name.function.macro.rules.quantum"
                },
                "3": {
                    "name": "entity.name.function.macro.quantum"
                },
                "4": {
                    "name": "entity.name.type.macro.quantum"
                },
                "5": {
                    "name": "punctuation.brackets.curly.quantum"
                }
            }
        },
        {
            "comment": "modules",
            "match": "(mod)\\s+((?:r#(?!crate|[Ss]elf|super))?[a-z][A-Za-z0-9_]*)",
            "captures": {
                "1": {
                    "name": "storage.type.quantum"
                },
                "2": {
                    "name": "entity.name.module.quantum"
                }
            }
        },
        {
            "comment": "external crate imports",
            "name": "meta.import.quantum",
            "begin": "\\b(extern)\\s+(crate)",
            "beginCaptures": {
                "1": {
                    "name": "storage.type.quantum"
                },
                "2": {
                    "name": "keyword.other.crate.quantum"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.quantum"
                }
            },
            "patterns": [
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#punctuation"
                }
            ]
        },
        {
            "comment": "use statements",
            "name": "meta.use.quantum",
            "begin": "\\b(use)\\s",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.quantum"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.quantum"
                }
            },
            "patterns": [
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#namespaces"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                },
                {
                    "include": "#lvariables"
                }
            ]
        },
        {
            "include": "#comments"
        },
        {
            "include": "#attributes"
        },
        {
            "include": "#lvariables"
        },
        {
            "include": "#constants"
        },
        {
            "include": "#gtypes"
        },
        {
            "include": "#functions"
        },
        {
            "include": "#types"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#lifetimes"
        },
        {
            "include": "#macros"
        },
        {
            "include": "#namespaces"
        },
        {
            "include": "#punctuation"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#variables"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [

                {
                    "comment": "line comments",
                    "name": "comment.line.double-slash.quantum",
                    "match": "(//).*$",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.comment.quantum"
                        }
                    }
                }
            ]
        },
        "constants": {
            "patterns": [
                {
                    "comment": "constant declarations",
                    "match": "\\b(const)\\s+([A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.quantum"
                        },
                        "2": {
                            "name": "constant.other.caps.quantum"
                        }
                    }
                },
                {
                    "comment": "decimal integers and floats",
                    "name": "constant.numeric.decimal.quantum",
                    "match": "\\b\\d[\\d_]*(\\.?)[\\d_]*(?:(E|e)([+-]?)([\\d_]+))?(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.separator.dot.decimal.quantum"
                        },
                        "2": {
                            "name": "keyword.operator.exponent.quantum"
                        },
                        "3": {
                            "name": "keyword.operator.exponent.sign.quantum"
                        },
                        "4": {
                            "name": "constant.numeric.decimal.exponent.mantissa.quantum"
                        },
                        "5": {
                            "name": "entity.name.type.numeric.quantum"
                        }
                    }
                },
                {
                    "comment": "booleans",
                    "name": "constant.language.bool.quantum",
                    "match": "\\b(true|false)\\b"
                }
            ]
        },
        "escapes": {
            "comment": "escapes: ASCII, byte, Unicode, quote, regex",
            "name": "constant.character.escape.quantum",
            "match": "(\\\\)(?:(?:(x[0-7][\\da-fA-F])|(u(\\{)[\\da-fA-F]{4,6}(\\}))|.))",
            "captures": {
                "1": {
                    "name": "constant.character.escape.backslash.quantum"
                },
                "2": {
                    "name": "constant.character.escape.bit.quantum"
                },
                "3": {
                    "name": "constant.character.escape.unicode.quantum"
                },
                "4": {
                    "name": "constant.character.escape.unicode.punctuation.quantum"
                },
                "5": {
                    "name": "constant.character.escape.unicode.punctuation.quantum"
                }
            }
        },
        "functions": {
            "patterns": [
                {
                    "comment": "pub as a function",
                    "match": "\\b(pub)(\\()",
                    "captures": {
                        "1": {
                            "name": "keyword.other.quantum"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.quantum"
                        }
                    }
                },
                {
                    "comment": "function definition",
                    "name": "meta.function.definition.quantum",
                    "begin": "\\b(fn)\\s+((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)((\\()|(<))",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.other.fn.quantum"
                        },
                        "2": {
                            "name": "entity.name.function.quantum"
                        },
                        "4": {
                            "name": "punctuation.brackets.round.quantum"
                        },
                        "5": {
                            "name": "punctuation.brackets.angle.quantum"
                        }
                    },
                    "end": "(\\{)|(;)",
                    "endCaptures": {
                        "1": {
                            "name": "punctuation.brackets.curly.quantum"
                        },
                        "2": {
                            "name": "punctuation.semi.quantum"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls, chaining",
                    "name": "meta.function.call.quantum",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.quantum"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.quantum"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.quantum"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#attributes"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls with turbofish",
                    "name": "meta.function.call.quantum",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(?=::<.*>\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.quantum"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.quantum"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#attributes"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "comment": "control flow keywords",
                    "name": "keyword.control.quantum",
                    "match": "\\b(await|break|continue|do|else|for|if|loop|match|return|try|while|yield)\\b"
                },
                {
                    "comment": "storage keywords",
                    "name": "keyword.other.quantum storage.type.quantum",
                    "match": "\\b(extern|let|macro|mod)\\b"
                },
                {
                    "comment": "const keyword",
                    "name": "storage.modifier.quantum",
                    "match": "\\b(const)\\b"
                },
                {
                    "comment": "type keyword",
                    "name": "keyword.declaration.type.quantum storage.type.quantum",
                    "match": "\\b(type)\\b"
                },
                {
                    "comment": "enum keyword",
                    "name": "keyword.declaration.enum.quantum storage.type.quantum",
                    "match": "\\b(enum)\\b"
                },
                {
                    "comment": "trait keyword",
                    "name": "keyword.declaration.trait.quantum storage.type.quantum",
                    "match": "\\b(trait)\\b"
                },
                {
                    "comment": "struct keyword",
                    "name": "keyword.declaration.struct.quantum storage.type.quantum",
                    "match": "\\b(struct)\\b"
                },
                {
                    "comment": "storage modifiers",
                    "name": "storage.modifier.quantum",
                    "match": "\\b(abstract|static)\\b"
                },
                {
                    "comment": "other keywords",
                    "name": "keyword.other.quantum",
                    "match": "\\b(as|async|become|box|dyn|move|final|gen|impl|in|override|priv|pub|ref|typeof|union|unsafe|unsized|use|virtual|where)\\b"
                },
                {
                    "comment": "fn",
                    "name": "keyword.other.fn.quantum",
                    "match": "\\bfn\\b"
                },
                {
                    "comment": "crate",
                    "name": "keyword.other.crate.quantum",
                    "match": "\\bcrate\\b"
                },
                {
                    "comment": "mut",
                    "name": "storage.modifier.mut.quantum",
                    "match": "\\bmut\\b"
                },
                {
                    "comment": "logical operators",
                    "name": "keyword.operator.logical.quantum",
                    "match": "(\\^|\\||\\|\\||&&|<<|>>|!)(?!=)"
                },
                {
                    "comment": "logical AND, borrow references",
                    "name": "keyword.operator.borrow.and.quantum",
                    "match": "&(?![&=])"
                },
                {
                    "comment": "assignment operators",
                    "name": "keyword.operator.assignment.quantum",
                    "match": "(\\+=|-=|\\*=|/=|%=|\\^=|&=|\\|=|<<=|>>=)"
                },
                {
                    "comment": "single equal",
                    "name": "keyword.operator.assignment.equal.quantum",
                    "match": "(?<![<>])=(?!=|>)"
                },
                {
                    "comment": "comparison operators",
                    "name": "keyword.operator.comparison.quantum",
                    "match": "(=(=)?(?!>)|!=|<=|(?<!=)>=)"
                },
                {
                    "comment": "math operators",
                    "name": "keyword.operator.math.quantum",
                    "match": "(([+%]|(\\*(?!\\w)))(?!=))|(-(?!>))|(/(?!/))"
                },
                {
                    "comment": "less than, greater than (special case)",
                    "match": "(?:\\b|(?:(\\))|(\\])|(\\})))[ \\t]+([<>])[ \\t]+(?:\\b|(?:(\\()|(\\[)|(\\{)))",
                    "captures": {
                        "1": {
                            "name": "punctuation.brackets.round.quantum"
                        },
                        "2": {
                            "name": "punctuation.brackets.square.quantum"
                        },
                        "3": {
                            "name": "punctuation.brackets.curly.quantum"
                        },
                        "4": {
                            "name": "keyword.operator.comparison.quantum"
                        },
                        "5": {
                            "name": "punctuation.brackets.round.quantum"
                        },
                        "6": {
                            "name": "punctuation.brackets.square.quantum"
                        },
                        "7": {
                            "name": "punctuation.brackets.curly.quantum"
                        }
                    }
                },
                {
                    "comment": "namespace operator",
                    "name": "keyword.operator.namespace.quantum",
                    "match": "::"
                },
                {
                    "comment": "dereference asterisk",
                    "match": "(\\*)(?=\\w+)",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.dereference.quantum"
                        }
                    }
                },
                {
                    "comment": "subpattern binding",
                    "name": "keyword.operator.subpattern.quantum",
                    "match": "@"
                },
                {
                    "comment": "dot access",
                    "name": "keyword.operator.access.dot.quantum",
                    "match": "\\.(?!\\.)"
                },
                {
                    "comment": "ranges, range patterns",
                    "name": "keyword.operator.range.quantum",
                    "match": "\\.{2}(=|\\.)?"
                },
                {
                    "comment": "colon",
                    "name": "keyword.operator.key-value.quantum",
                    "match": ":(?!:)"
                },
                {
                    "comment": "dashrocket, skinny arrow",
                    "name": "keyword.operator.arrow.skinny.quantum",
                    "match": "->|<-"
                },
                {
                    "comment": "hashrocket, fat arrow",
                    "name": "keyword.operator.arrow.fat.quantum",
                    "match": "=>"
                },
                {
                    "comment": "dollar macros",
                    "name": "keyword.operator.macro.dollar.quantum",
                    "match": "\\$"
                },
                {
                    "comment": "question mark operator, questionably sized, macro kleene matcher",
                    "name": "keyword.operator.question.quantum",
                    "match": "\\?"
                }
            ]
        },
        "interpolations": {
            "comment": "curly brace interpolations",
            "name": "meta.interpolation.quantum",
            "match": "({)[^\"{}]*(})",
            "captures": {
                "1": {
                    "name": "punctuation.definition.interpolation.quantum"
                },
                "2": {
                    "name": "punctuation.definition.interpolation.quantum"
                }
            }
        },
        "lifetimes": {
            "patterns": [
                {
                    "comment": "named lifetime parameters",
                    "match": "(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.lifetime.quantum"
                        },
                        "2": {
                            "name": "entity.name.type.lifetime.quantum"
                        }
                    }
                },
                {
                    "comment": "borrowing references to named lifetimes",
                    "match": "(\\&)(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.borrow.quantum"
                        },
                        "2": {
                            "name": "punctuation.definition.lifetime.quantum"
                        },
                        "3": {
                            "name": "entity.name.type.lifetime.quantum"
                        }
                    }
                }
            ]
        },
        "macros": {
            "patterns": [
                {
                    "comment": "macros",
                    "name": "meta.macro.quantum",
                    "match": "(([a-z_][A-Za-z0-9_]*!)|([A-Z_][A-Za-z0-9_]*!))",
                    "captures": {
                        "2": {
                            "name": "entity.name.function.macro.quantum"
                        },
                        "3": {
                            "name": "entity.name.type.macro.quantum"
                        }
                    }
                }
            ]
        },
        "namespaces": {
            "patterns": [
                {
                    "comment": "namespace (non-type, non-function path segment)",
                    "match": "(?<![A-Za-z0-9_])([A-Za-z0-9_]+)((?<!super|self)::)",
                    "captures": {
                        "1": {
                            "name": "entity.name.namespace.quantum"
                        },
                        "2": {
                            "name": "keyword.operator.namespace.quantum"
                        }
                    }
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "comment": "numeric types",
                    "match": "(?<![A-Za-z])(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.quantum"
                        }
                    }
                },
                {
                    "comment": "parameterized types",
                    "begin": "\\b(_?[A-Z][A-Za-z0-9_]*)(<)",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.type.quantum"
                        },
                        "2": {
                            "name": "punctuation.brackets.angle.quantum"
                        }
                    },
                    "end": ">",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.angle.quantum"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "primitive types",
                    "name": "entity.name.type.primitive.quantum",
                    "match": "\\b(bool|char|str)\\b"
                },
                {
                    "comment": "trait declarations",
                    "match": "\\b(trait)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.trait.quantum storage.type.quantum"
                        },
                        "2": {
                            "name": "entity.name.type.trait.quantum"
                        }
                    }
                },
                {
                    "comment": "struct declarations",
                    "match": "\\b(struct)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.struct.quantum storage.type.quantum"
                        },
                        "2": {
                            "name": "entity.name.type.struct.quantum"
                        }
                    }
                },
                {
                    "comment": "enum declarations",
                    "match": "\\b(enum)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.enum.quantum storage.type.quantum"
                        },
                        "2": {
                            "name": "entity.name.type.enum.quantum"
                        }
                    }
                },
                {
                    "comment": "type declarations",
                    "match": "\\b(type)\\s+(_?[A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.declaration.type.quantum storage.type.quantum"
                        },
                        "2": {
                            "name": "entity.name.type.declaration.quantum"
                        }
                    }
                },
                {
                    "comment": "types",
                    "name": "entity.name.type.quantum",
                    "match": "\\b_?[A-Z][A-Za-z0-9_]*\\b(?!!)"
                }
            ]
        },
        "punctuation": {
            "patterns": [
                {
                    "comment": "comma",
                    "name": "punctuation.comma.quantum",
                    "match": ","
                },
                {
                    "comment": "curly braces",
                    "name": "punctuation.brackets.curly.quantum",
                    "match": "[{}]"
                },
                {
