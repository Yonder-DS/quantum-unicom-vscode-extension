{
  "name": "quantum-configuration-sample",
  "displayName": "Quantum Configuration",
  "description": "Quantum Configuration",
  "version": "0.0.1",
  "publisher": "ryanc",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "quantum-icon.png",
  "contributes": {
    "languages": [
      {
        "id": "quantum",
        "extensions": [".qin", ".qax"],
        "filenames": ["axes", "tabs"],
        "filenamePatterns": ["run*", "break*", "edit*", "base*", "mean*", "brk*", "data*"],
        "aliases": [
          "Quantum",
          "qt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [{
        "language": "quantum",
        "scopeName": "source.quantum",
        "path": "./syntaxes/quantum.tmLanguage.json"
    }]
  }
}
